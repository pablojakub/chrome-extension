{
  "name": "React bookmarks",
  "description": "Save bookmarks and have insight into history",
  "version": "1.3",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open"
  },
  "icons": {
    "16": "bookmark16.png",
    "48": "bookmark48.png",
    "128": "bookmark128.png"
  },
  "permissions": [
    "tabs",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "./static/js/contentScript.js"
      ]
    }
  ]
}